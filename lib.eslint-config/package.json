    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsdoc": "^46.2.4",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-no-inferred-method-name": "^2.0.0",
    "eslint-plugin-prettier": "^4.2.1",
