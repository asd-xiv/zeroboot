{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": false,
  "quoteProps": "consistent",
  "jsxSingleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": true,
  "arrowParens": "avoid",
  "proseWrap": "always",
  "endOfLine": "lf",

  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^core.libs/(.*)$",
    "^core.types/(.*)$",
    "^core.hooks/(.*)$",
    "^core.i18n/(.*)$",
    "^core.ui/(.*)$",
    "^(page.(.*)|route.(.*)|layout.(.*))/(.*)$",
    "^(.*)test-mock(.*)$",
    "^./data/(.*)$",
    "^./section/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true
}
